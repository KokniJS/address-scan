{
  "name": "@my-workspace/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "api:local": "yarn nx run eth-scan:serve",
    "api:migration:generate": "yarn nx run eth-scan:generate-migration",
    "typeorm:cli": "ts-node -P apps/eth-scan/tsconfig.app.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn run typeorm:cli -- migration:generate -d ./apps/eth-scan/src/config/db.migration.ts -o ./apps/eth-scan/src/migrations/$N",
    "migration:run": "yarn typeorm:cli migration:run -d apps/eth-scan/src/config/db.migration.ts",
    "migration:revert": "yarn typeorm:cli migration:revert -d apps/eth-scan/src/config/db.migration.ts"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.0.2",
    "@nestjs/platform-fastify": "^10.3.2",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/typeorm": "^10.0.2",
    "class-transformer": "^0.5.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/eslint": "18.0.3",
    "@nx/eslint-plugin": "18.0.3",
    "@nx/jest": "18.0.3",
    "@nx/js": "18.0.3",
    "@nx/nest": "18.0.3",
    "@nx/node": "18.0.3",
    "@nx/web": "18.0.3",
    "@nx/webpack": "18.0.3",
    "@nx/workspace": "18.0.3",
    "@swc-node/register": "~1.6.7",
    "@swc/core": "~1.3.85",
    "@swc/helpers": "~0.5.2",
    "@types/jest": "^29.4.0",
    "@types/node": "~18.16.9",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "~8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "nx": "18.0.3",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.3.2",
    "webpack-cli": "^5.1.4"
  }
}
